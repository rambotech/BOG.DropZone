{
	"Logging": {
		"IncludeScopes": false,
		"Debug": {
			"LogLevel": {
				"Default": "Warning"
			}
		},
		"Console": {
			"LogLevel": {
				"Default": "Warning"
			}
		}
	},
	"LettuceEncrypt": {
		"AcceptTermsOfService": "true",
		"EmailAddress": "you@yourdomain.com",
		"UseStagingServer": "true",
		"Domains": [
			"www.mywebserver.com",
			"mywebserver.com"
		],
		"RenewalCheckPeriod": 6,
		"RenewDaysInAdvance": 2,
	},
	"MaxPayloadCount": "100",
	"MaxPayloadSize": "536870912",
	"MaxReferencesCount": "50",
	"MaxReferenceSize": "1073741824",
	"AccessToken": "YourAccessValueHere",
	"AdminToken": "YourAdminValueHere",
	"MaxDropzones": 5,
	"MaximumFailedAttemptsBeforeLockout": 3,
	"LockoutSeconds": 300,
	"httpPort": 5000,
	"HttpsPort": 0,
	"UseLetsEncrypt": false
}
